{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "extends": "nx/presets/npm.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "publish": {
      "dependsOn": ["build", "^publish"]
    },
    "lint": {
      "dependsOn": ["build"]
    }
  },
  "defaultBase": "main"
}
