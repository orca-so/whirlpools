{
  "name": "@orca-so/whirlpools-lint",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "format": "cd ../.. && prettier './**/*.{js,jsx,ts,tsx,json}' --write && eslint -c ts-sdk/lint/eslint.config.js --fix",
    "lint": "cd ../.. && prettier --check './**/*.{js,jsx,ts,tsx,json}' && eslint -c ts-sdk/lint/eslint.config.js"
  },
  "devDependencies": {
    "eslint": "^9.20.1",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
}
