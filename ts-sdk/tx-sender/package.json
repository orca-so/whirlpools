{
  "name": "@orca-so/tx-sender",
  "version": "0.1.0",
  "description": "Send transactions to the Solana blockchain with auto priority fees.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "../../LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "test": "vitest run tests",
    "deploy": "../../scripts/deploy-npm",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@solana-program/address-lookup-table": "^0.6.1",
    "@solana-program/compute-budget": "^0.6.1",
    "@solana-program/system": "^0.6.2"
  },
  "peerDependencies": {
    "@solana/web3.js": "^2.0.0"
  },
  "devDependencies": {
    "@solana/web3.js": "^2.0.0",
    "tsup": "^8.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orca-so/whirlpools.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "solana",
    "crypto",
    "defi",
    "dex",
    "amm"
  ],
  "author": "team@orca.so",
  "bugs": {
    "url": "https://github.com/orca-so/whirlpools/issues"
  },
  "homepage": "https://orca.so"
}
